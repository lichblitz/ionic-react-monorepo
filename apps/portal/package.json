    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-boundaries": "^2.0.0",
